{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "on",
    "files.trimTrailingWhitespace": false
  },
  "[mdx]": {
    "editor.wordWrap": "on",
    "files.trimTrailingWhitespace": false
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": true,
  "bundleSize.cautionColor": "#eeaa0022",
  "chat.agent.enabled": true,
  "colorInfo.languages": [
    {
      "colors": "css",
      "selector": "css"
    },
    {
      "colors": "css",
      "selector": "javascriptreact"
    },
    {
      "colors": "css",
      "selector": "javascript"
    }
  ],
  "dbcode.connections": [
    {
      "color": "#FFC54F",
      "connectionId": "-0BLGkpE0CYh8ubxjhMqa",
      "connectionType": "socket",
      "driver": "sqlite",
      "driverOptions": {
        "extensions": []
      },
      "name": "Lexin",
      "readOnly": false,
      "socket": "/Users/ABARGI/Desktop/lexin.db"
    }
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderIndicators": false,
  "diffEditor.renderSideBySide": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "SF Mono, SF Arabic",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.guides.indentation": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 21,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "gutter",
  "editor.selectionHighlight": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "typescript": "jsx",
    "typescriptreact": "jsx"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": false,
  "files.associations": {
    "*.note": "markdown",
    ".env": "dotenv"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "markdown": true
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "gitlens.blame.avatars": false,
  "gitlens.blame.highlight.locations": ["gutter", "overview"],
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.mode.active": "review",
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.views.repositories.branches.layout": "list",
  "gitlens.views.repositories.files.layout": "list",
  "hediet.vscode-drawio.local-storage": "",
  "hediet.vscode-drawio.resizeImages": null,
  "hediet.vscode-drawio.theme": "kennedy",
  "html.format.wrapAttributes": "force",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.checkJs": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredSettings": [],
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontSize": 14,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescriptExplorer.typeTree.view.icons.colors.enable": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.commandCenter": false,
  "window.title": "${rootName}",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {},
  "workbench.colorTheme": "PlasticAB Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "symbols",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.sideBar.location": "left",
  "workbench.tree.renderIndentGuides": "none",
  "@azure.argTenant": "",
  "remote.defaultExtensionsIfInstalledLocally": [],
  "dbcode.cloudProviders": [],
  "chat.agent.maxRequests": 50,
  "chat.renderRelatedFiles": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "terminal.integrated.cursorStyleInactive": "none",
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "terminal.integrated.enableImages": true,
  "dbcode.ai.inlineCompletion": false
}
